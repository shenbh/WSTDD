<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="main_bottom_radiobottom">
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
    </style>

    <style name="user_face_image">
        <item name="android:layout_width">40.0dip</item>
        <item name="android:layout_height">40.0dip</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:padding">1.0dip</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="MyApplyListCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/applylistcheck</item>
        <item name="android:layout_margin">10dp</item>
    </style>
    <!-- 控件的长 宽start -->
    <style name="layout_ww_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layout_1m_style">
        <item name="android:layout_width">0.1dip</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="layout_mm_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="layout_mw_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="layout_wm_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="pop_anim">
        <item name="android:windowEnterAnimation">@anim/translate</item>
        <item name="android:windowExitAnimation">@anim/translate_out</item>
    </style>

    <style name="layout_mW_4H_style">
